@model List<CourseData>
@{
    ViewData["Title"] = "CourseDetails";
}

<table class="table table-hover table-bordered" style="margin-bottom=20px">
    @foreach(var item in Model)
    {
        <tr>
            <td>
                @item.course_result.Course.Name
            </td>
        </tr>
        break;
    }
</table>

<table class="table table-hover table-bordered">
    <tr>
        <td>Trainee Name </td>
        <td>Trainee degree</td>
        <td>Mindgree</td>
        <td>Maxdgree</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr style="color:@item.color" >
            <td>@item.course_result.Trainee.Name</td>
            <td>@item.course_result.DgreeOfTrainee</td>
            <td>@item.course_result.Course.MinDgree</td>
            <td>@item.course_result.Course.MaxDgree</td>
        </tr>
    }
</table>